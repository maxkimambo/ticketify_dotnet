@model IEnumerable<Ticket.Domain.Route>
<div class="panel panel-green margin-bottom-40">
    <div class="panel-heading">
        <h3 class="panel-title"><i class="fa fa-home"></i> Routes </h3>
    </div>
    <div class="panel-body">
        <table id="routes" class="table table-condensed table-hover table-striped">
            <thead>
                <tr>
                    <th>Start</th>
                    <th>Destination</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var loc in Model)
                {
                    <tr>
                        <td> @loc.Start.Name </td>
                        <td> @loc.Destination.Name </td>
                        <td><a href="#"> <i class="fa fa-edit"></i> </a> </td>
                    </tr>
                }

            </tbody>
        </table>
    </div>
</div>



